{
  "name": "express-movies-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint --config .eslintrc.js . --fix",
    "format": "prettier --write \"./**/*.js\" \"./**/**/*.js\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-google": "^0.14.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,scss,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}